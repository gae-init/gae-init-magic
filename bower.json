{
  "name": "gae-init",
  "dependencies": {
    "bootstrap": "~3",
    "bootswatch": "~3",
    "font-awesome": "~4",
    "highlight.js": "https://github.com/isagalaev/highlight.js.git#8.4",
    "jquery": "~2",
    "moment": "~2",
    "selectize": "~0"
  },
  "overrides": {
    "bootstrap": {
      "main": ["less/**", "fonts/*", "js/*"]
    },
    "bootswatch": {
      "main": ["**/*.less"]
    },
    "font-awesome": {
      "main": ["less/*", "fonts/*"]
    },
    "highlight.js": {
      "main": ["src/**/*.{js,css}"]
    },
    "selectize": {
      "main": ["dist/**/*.{js,less}"]
    }
  }
}
